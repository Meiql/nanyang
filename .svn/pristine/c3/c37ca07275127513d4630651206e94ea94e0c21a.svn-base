<#ftl output_format="HTML" auto_esc=true>
<@h.commonHead title="后台管理系统" keywords="开源,永久免费" description="springrain开源系统管理后台"/>


</head>
<body>
	<div class="layui-layout layui-layout-admin">
		<@h.naviHeader /><@h.leftMenu />
		<!-- 主体内容开始 -->
			<div class="layui-tab layui-tab-brief">
				<ul class="layui-tab-title site-demo-title">
		            <li class="layui-this">
		            </li>
		             <li style="float:right;">
				        <button type="button" onclick="history.go(-1)" class="layui-btn layui-btn-small" style="margin-top:8px;"><i class="layui-icon layui-icon-specil">&#xe603;</i>返回</button>
		             </li>
	       		</ul>
				
				<div class="layui-body layui-tab-content site-demo-body">
					<div class="layui-tab-item layui-show">
							<div class="layui-main">
						          <div id="LAY_preview" class="layui-my-form">
						          				<header class="larry-personal-tit">
													<span>添加资产领用记录表</span>
												</header>
												<div class="larry-personal-body clearfix changepwd">
													<form id="validForm" class="layui-form <!--  -->"  method="post" action="${ctx}/zichanrecord/update">
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">zcid*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.zcid)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">lingid*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.lingid)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">发生类型*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.retype)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">发生人*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.reuser)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">发生人id*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.reuserid)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">发生部门*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.reunit)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">发生部门id*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.reunitid)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<!--日期型-->
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">发生日期*</label>
																		<div class="layui-inline col-lg-5">  
																			${((returnDatas.data.redate)?string('yyyy-MM-dd'))!'' }
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">变动数量*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.renum)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">变动金额*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.remoney)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">备注*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.remark)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">创建人*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.createuser)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<!--日期型-->
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">创建日期*</label>
																		<div class="layui-inline col-lg-5">  
																			${((returnDatas.data.createtime)?string('yyyy-MM-dd'))!'' }
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">active*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.active)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">审核人*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.bak1)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">bak2*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.bak2)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
																	<div class="layui-form-item col-lg-6">
																		<label class="layui-form-label">bak3*</label>
																		<div class="layui-inline col-lg-5">  
																			${(returnDatas.data.bak3)!''}
																		</div>
																		<div class="layui-inline valid-info"></div>
																	</div>
													</form>
												</div>
								  </div>
							</div>
					</div>
			</div>
		</div>
		<!-- 主体内容结束 -->
		<@h.footer />
	</div>
</body>
</html>